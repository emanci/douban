{
    "name": "emanci/douban",
    "description": "PHP SDK for http://www.douban.com",
    "keywords": ["douban", "douban-movie", "douban-book", "douban-music", "douban-fm"],
    "license": "MIT",
    "authors": [{
        "name": "emanci",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Emanci\\Douban\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
